{
  "compilerOptions": {
    "target": "es6",

    /* Modules */
    "module": "CommonJS",

    /* Emit */
    "sourceMap": true,
    "outDir": "dist",

    /* Interop Constraints */
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    /* Type Checking */
    "strict": true,
    "skipLibCheck": true,
  }
}
